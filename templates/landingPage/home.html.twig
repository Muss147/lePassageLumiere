{% block title %}Hello FrontController!{% endblock %}

{% block stylesheets %}
<style>
    
</style>
{% endblock %}

{% block body %}
<section id="carrousel" class="carrousel">
    <div class="carrousel__div" style="background: url({{asset('assets/medias/bg/homeCover.png')}}) top / cover no-repeat;">
        <div class="container grid-container" style="padding: 0;">
            <div class="carrousel__title">
                <h1><span class="fw-100">Des </span>commerces <br><span class="fw-100">en plein coeur de Zone 4</span></h1>
                <p>Livraison : 2<sup>e</sup> semestre 2025</p>
            </div>
            <div class="carrousel__form-div d-flex-end">
                <div>
                    <div class="d-flex-end mb-2">
                        <span>Développé par</span>
                        <img src="{{ asset('assets/medias/logos/logoPFO_white.png') }}" alt="PFO" height="45">
                    </div>
                    <form action="" method="post" class="carrousel__form">
